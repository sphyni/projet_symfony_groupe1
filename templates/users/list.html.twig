{% extends 'layout.html.twig' %}
{% block title_page %}
    Liste des utilisateurs du site !
{% endblock %}
{% block main %}
    <p> username : {{ app.user.username }}</p>
    <ul>
        {% for user in users %}
            <li><a href="{{ url("profile", {'id':user.getId()}) }}">{{ user.getPrenom() }} {{ user.getNom() }}</a> </li>
        {% endfor %}
    </ul>
{% endblock %}